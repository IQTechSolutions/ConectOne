    <script>
        window.setDragImageToRow = (elementId) => {
            const rowElement = document.getElementById(elementId);
            if (rowElement) {
                rowElement.addEventListener('dragstart', (event) => {
                    event.dataTransfer.setDragImage(rowElement, 0, 0); // Set the row as the drag image
                });
            }
        };

        function ScrollTo(elementId) {
            var element = document.getElementById(elementId);

            if (element) {
                element.scrollIntoView({
                    behavior: 'smooth'
                });
            }
        }
    </script>

